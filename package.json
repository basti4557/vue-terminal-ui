{
  "name": "vue-terminal-ui",
  "version": "0.1.1",
  "description": "terminal UI for VueJs",
  "main": "src/VueTerminal.vue",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "eslint --ext .vue src",
    "build": "webpack"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.12",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.12",
    "@vue/cli-service": "^3.0.0-beta.12",
    "vue-template-compiler": "^2.5.16",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "uglify-es-webpack-plugin": "^0.10.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.4.1"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
